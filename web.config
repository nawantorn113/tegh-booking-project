<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <clear />
            <add name="Python FastCGI"
                 path="*"
                 verb="*"
                 modules="FastCgiModule"
                 scriptProcessor="C:\inetpub\wwwroot\tegh-booking-project\.venv\Scripts\python.exe|C:\inetpub\wwwroot\tegh-booking-project\.venv\lib\site-packages\wfastcgi.py"
                 resourceType="Unspecified"
                 requireAccess="Script" />
        </handlers>
    </system.webServer>

    <appSettings>
        <add key="PYTHONPATH" value="C:\inetpub\wwwroot\tegh-booking-project" />

        <add key="WSGI_HANDLER" value="mysite.wsgi.application" />
        <add key="DJANGO_SETTINGS_MODULE" value="mysite.settings" />
    </appSettings>
</configuration>